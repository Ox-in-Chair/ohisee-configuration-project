{
  "name": "ohisee-reports",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3008",
    "build": "next build",
    "build:analyze": "ANALYZE=true npm run build",
    "start": "next start",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "lint:check": "eslint . --max-warnings 0",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:e2e": "jest --config jest.e2e.config.js --runInBand",
    "test:e2e:headed": "E2E_HEADED=true jest --config jest.e2e.config.js --runInBand",
    "test:e2e:debug": "E2E_HEADED=true E2E_SLOW_MO=500 E2E_DEBUG_AI=true jest --config jest.e2e.config.js --runInBand --verbose",
    "test:e2e:video": "E2E_VIDEO=true jest --config jest.e2e.config.js --runInBand",
    "test:e2e:local": "npm run dev & sleep 5 && npm run test:e2e:headed; kill %1",
    "test:all": "npm run test && npm run test:e2e",
    "type-check": "tsc --noEmit",
    "type-check:strict": "tsc --noEmit --strict",
    "validate": "npm run type-check && npm run lint",
    "validate:strict": "npm run type-check:strict && npm run lint",
    "precommit": "npm run validate && npm run test -- --bail",
    "upload-procedures": "tsx scripts/upload-procedures.ts",
    "upload-kangopak": "tsx scripts/upload-all-kangopak-procedures.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/supabase-js": "^2.79.0",
    "@tailwindcss/typography": "^0.5.19",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "lucide-react": "^0.552.0",
    "next": "16.0.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.66.0",
    "recharts": "^3.4.1",
    "resend": "^4.8.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@browserbasehq/stagehand": "^3.0.1",
    "@jest/globals": "^30.2.0",
    "@next/bundle-analyzer": "^16.0.1",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "playwright": "^1.56.1",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
